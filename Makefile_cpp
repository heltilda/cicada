CFLAGS = -O3
CC = g++
OBJ = lnklst.o cmpile.o cicada.o intrpt.o bytecd.o ciclib.o userfn.o ccmain.o

cicada: $(OBJ)
	$(CC) $(CFLAGS) -o cicada $(OBJ) -lm

lnklst.o: lnklst.h lnklst
cmpile.o: lnklst.h cmpile.h cmpile
cicada.o: lnklst.h cmpile.h cicada.h cicada
intrpt.o: lnklst.h cmpile.h cicada.h intrpt.h bytecd.h userfn.h intrpt
bytecd.o: lnklst.h cmpile.h cicada.h intrpt.h bytecd.h ciclib.h userfn.h bytecd
ciclib.o: lnklst.h cmpile.h cicada.h intrpt.h bytecd.h ciclib.h userfn.h ccmain.h ciclib
userfn.o: lnklst.h intrpt.h userfn.h ccmain.h userfn
ccmain.o: lnklst.h cmpile.h cicada.h intrpt.h bytecd.h ciclib.h userfn.h ccmain.h ccmain
